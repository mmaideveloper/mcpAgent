    "version": "2.0.0",
    "tasks": [
        {
            "label": "Debug C# Project",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ai-multi-agent/ai-multi-agent.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"             
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },      
            "problemMatcher": "$msCompile"
        }
    ]
}